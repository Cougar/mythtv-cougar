<myththeme>

  <background style="normal">
    <buttonspread>yes</buttonspread>
    <balancerows>no</balancerows>
    <buttonarea>15,155,1250,400</buttonarea>
    <visiblerowlimit>6</visiblerowlimit>
    <columns>6</columns>
  </background>

  <titles>
    <position>0,80</position>
    <image mode="TV">title/title_tv.png</image>
    <image mode="MAIN">title/title_main.png</image>
    <image mode="MUSIC">title/title_music.png</image>
    <image mode="GALLERY">title/title_gallery.png</image>
    <image mode="VIDEO">title/title_video.png</image>
    <image mode="SCHEDULE">title/title_schedule.png</image>
    <image mode="SEARCH">title/title_search.png</image>
    <image mode="SETUP">title/title_setup.png</image>
    <image mode="MEDIA_SETUP">title/title_media_settings.png</image>
    <image mode="INFO_SETUP">title/title_info_setup.png</image>
    <image mode="TVSETUP">title/title_tv_setup.png</image>
    <image mode="DVD">title/title_dvd.png</image>
    <image mode="DVD_SETTINGS">title/title_dvd_setup.png</image>
    <image mode="MUSIC_SETTINGS">title/title_music_setup.png</image>
    <image mode="VIDEO_SETTINGS">title/title_video_setup.png</image>
    <image mode="GAME_SETTINGS">title/title_game_setup.png</image>
    <image mode="LIBRARY">title/title_library.png</image>
    <image mode="MANAGE_RECORDINGS">title/title_manage_recordings.png</image>
    <image mode="INFO">title/title_info_center.png</image>
    <image mode="OPTICAL_DISK">title/title_optical_disks.png</image>
    <image mode="UTIL">title/title_util.png</image>
  </titles>

  <genericbutton>
    <watermarkposition>880,115</watermarkposition> <!-- 20px right margin -->
    <normal>button_off.png</normal>
    <active>button_off.png</active>
    <text>
      <area>0,95,0,0</area>
      <color>#8AAAF8</color>
      <fontname>Freesans</fontname>
      <fontsize>21</fontsize>
      <bold>yes</bold>
      <italics>no</italics>
      <halign>center</halign>
      <outline>
         <color>#41517D</color>
         <size>1</size>
      </outline>
      <shadow>
         <color>black</color>
         <offset>2,2</offset>
         <alpha>255</alpha>
      </shadow>
    </text>
    <activetext>
    <area>0,95,0,0</area>
      <color>#D1F1FF</color>
      <fontname>Freesans</fontname>
      <fontsize>21</fontsize>
      <bold>yes</bold>
      <italics>no</italics>
      <halign>center</halign>
    <outline>
         <color>#41517D</color>
         <size>2</size>
      </outline>
      <!--<shadow>
         <color>black</color>
         <offset>3,3</offset>
         <alpha>128</alpha>
      </shadow>-->
    </activetext>
  </genericbutton>


  <uparrow>
    <image>shared/up_arrow.png</image>
    <position>65,120</position>
  </uparrow>

  <downarrow>
    <image>shared/down_arrow.png</image>
    <position>65,460</position>
  </downarrow>


