  <genericbutton>
    <watermarkposition>880,115</watermarkposition> <!-- 20px right margin -->
    <normal>button_off.png</normal>
    <active>button_off.png</active>
    <text>
      <area>0,0,0,0</area>
      <color>#8AAAF8</color>
      <fontname>Freesans</fontname>
      <fontsize>21</fontsize>
      <bold>yes</bold>
      <italics>no</italics>
      <halign>center</halign>
      <outline>
         <color>#41517D</color>
         <size>1</size>
      </outline>
      <shadow>
         <color>black</color>
         <offset>2,2</offset>
         <alpha>255</alpha>
      </shadow>
    </text>
    <activetext>
    <area>0,0,0,0</area>
      <color>#D1F1FF</color>
      <fontname>Freesans</fontname>
      <fontsize>21</fontsize>
      <bold>yes</bold>
      <italics>no</italics>
      <halign>center</halign>
    <outline>
         <color>#41517D</color>
         <size>2</size>
      </outline>
      <!--<shadow>
         <color>black</color>
         <offset>3,3</offset>
         <alpha>128</alpha>
      </shadow>-->
    </activetext>
  </genericbutton>
  

  <uparrow>
    <image>shared/up_arrow.png</image>
    <position>65,120</position>
  </uparrow>

  <downarrow>
    <image>shared/down_arrow.png</image>
    <position>65,460</position>
  </downarrow>

  
