        <buttonlist name="menu">
            <area>95,120,470,470</area>
            <layout>vertical</layout>
            <spacing>9</spacing>
            <buttonarea>20,32,300,410</buttonarea>
            <statetype name="buttonitem">
                <state name="active">
                    <area>0,0,300,50</area>
                    <textarea name="buttontext">
                        <area>0,0,300,50</area>
                        <font>menufont</font>
                        <align>left,vcenter</align>
                        <cutdown>yes</cutdown>
                    </textarea>
                </state>
                <state name="selected" from="active">
                    <textarea name="buttontext">
                        <font>menufontselected</font>
                    </textarea>
                </state>
            </statetype>
            <statetype name="upscrollarrow">
                <position>0,0</position>
                <showempty>yes</showempty>
                <state type="full">
                    <imagetype name="upon">
                        <filename>arrow_up.png</filename>
                    </imagetype>
                </state>
            </statetype>
            <statetype name="downscrollarrow">
                <position>0,432</position>
                <showempty>yes</showempty>
                <state type="full">
                    <imagetype name="dnon">
                        <filename>arrow_down.png</filename>
                    </imagetype>
                </state>
            </statetype>
        </buttonlist>

