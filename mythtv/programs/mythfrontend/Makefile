CXXFLAGS = -pipe -ffunction-sections -Wpointer-arith -Wwrite-strings -Wmissing-prototypes -Wall -W -O2 -D_REENTRANT  -D_GNU_SOURCE -DQT_NO_DEBUG -DQT_THREAD_SUPPORT
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I../epg -I../dialog -I../tv -I../tv/libNuppelVideo -I/usr/include/mysql -I$(QTDIR)/include -I$(QTDIR)/mkspecs/linux-g++
