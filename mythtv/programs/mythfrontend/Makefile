LIBS     = $(SUBLIBS) -L$(QTDIR)/lib -L/usr/X11R6/lib -L../tv -L../tv/libNuppelVideo -lmythtv -lNuppelVideo `../tv/libNuppelVideo/nuppelvideo-config --libs` `mysql_config --libs` -lqt-mt -lpthread -lXext -lX11 -lm
AR       = ar cqs
RANLIB   = 
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic
QMAKE    = $(QTDIR)/bin/qmake
