####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: mythtv.pro  $(QTDIR)/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) mythtv.pro
qmake: 
	@$(QMAKE) mythtv.pro

dist: 
	@mkdir -p .tmp/mythfrontend && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/mythfrontend/ && ( cd `dirname .tmp/mythfrontend` && $(TAR) mythfrontend.tar mythfrontend && $(GZIP) mythfrontend.tar ) && mv `dirname .tmp/mythfrontend`/mythfrontend.tar.gz . && rm -rf .tmp/mythfrontend

mocclean:
