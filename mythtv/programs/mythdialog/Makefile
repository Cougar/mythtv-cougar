CXXFLAGS = -pipe -ffunction-sections -Wpointer-arith -Wwrite-strings -Wmissing-prototypes -Wall -W -O6 -march=pentiumpro -fomit-frame-pointer -funroll-loops -fexpensive-optimizations -finline-functions -D_REENTRANT  -D_GNU_SOURCE -DMMX -DQT_NO_DEBUG -DQT_THREAD_SUPPORT
