.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: mythdialog.pro  $(QTDIR)/mkspecs/linux-g++/qmake.conf ../settings.pro
	$(QMAKE) mythdialog.pro
qmake: 
	@$(QMAKE) mythdialog.pro

dist: 
	@mkdir -p .tmp/mythdialog && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/mythdialog/ && ( cd `dirname .tmp/mythdialog` && $(TAR) mythdialog.tar mythdialog && $(GZIP) mythdialog.tar ) && mv `dirname .tmp/mythdialog`/mythdialog.tar.gz . && rm -rf .tmp/mythdialog

mocclean:
	-rm -f $(OBJMOC)
	-rm -f $(SRCMOC)

uiclean:

clean: mocclean
	-rm -f $(OBJECTS) 
	-rm -f *~ core *.core


####### Sub-libraries

