#############################################################################
# Makefile for building: mythweather
# Generated by qmake (1.04a) (Qt 3.1.1) on: Mon Mar  3 17:46:35 2003
# Project:  mythweather.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile mythweather.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
GUNZIP   =      gunzip -c
SUBTARGETS =	 \
		sub-mythweather

first: all

all: Makefile $(SUBTARGETS)

mythweather/$(MAKEFILE): 
	cd mythweather && (gunzip -c accid.dat.gz > accid.dat) && $(QMAKE)  -o $(MAKEFILE)
sub-mythweather: mythweather/$(MAKEFILE) FORCE
	cd mythweather && (gunzip -c accid.dat.gz > accid.dat) && $(MAKE) -f $(MAKEFILE)

Makefile: mythweather.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile mythweather.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile mythweather.pro

all: $(SUBTARGETS)
qmake_all: mythweather/$(MAKEFILE)
	 ( [ -d mythweather ] && cd mythweather ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d mythweather ] && cd mythweather ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d mythweather ] && cd mythweather ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

FORCE:

