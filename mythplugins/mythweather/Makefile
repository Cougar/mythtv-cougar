MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-mythweather \
		sub-i18n

first: all

all: Makefile $(SUBTARGETS)

mythweather/$(MAKEFILE): 
	cd mythweather && $(QMAKE)  -o $(MAKEFILE)
sub-mythweather: mythweather/$(MAKEFILE) FORCE
	cd mythweather && $(MAKE) -f $(MAKEFILE)

i18n/$(MAKEFILE): 
	cd i18n && $(QMAKE)  -o $(MAKEFILE)
sub-i18n: i18n/$(MAKEFILE) FORCE
	cd i18n && $(MAKE) -f $(MAKEFILE)

Makefile: mythweather.pro  $(QTDIR)/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile mythweather.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile mythweather.pro

all: $(SUBTARGETS)
qmake_all: mythweather/$(MAKEFILE) i18n/$(MAKEFILE)
	 ( [ -d mythweather ] && cd mythweather ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d i18n ] && cd i18n ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d mythweather ] && cd mythweather ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d i18n ] && cd i18n ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d mythweather ] && cd mythweather ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d i18n ] && cd i18n ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

FORCE:

