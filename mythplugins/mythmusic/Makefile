MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-mythmusic

first: all

all: Makefile $(SUBTARGETS)

mythmusic/$(MAKEFILE): 
	cd mythmusic && $(QMAKE)  -o $(MAKEFILE)
sub-mythmusic: mythmusic/$(MAKEFILE) FORCE
	cd mythmusic && $(MAKE) -f $(MAKEFILE)

Makefile: mythmusic.pro  $(QTDIR)/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile mythmusic.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile mythmusic.pro

all: $(SUBTARGETS)
qmake_all: mythmusic/$(MAKEFILE)
	 ( [ -d mythmusic ] && cd mythmusic ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d mythmusic ] && cd mythmusic ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d mythmusic ] && cd mythmusic ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

FORCE:

