#############################################################################
# Makefile for building: mythnews
# Generated by qmake (1.06c) (Qt 3.2.1) on: Mon Sep  8 08:21:26 2003
# Project:  mythnews.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile mythnews.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
SUBTARGETS =	 \
		sub-mythnews

first: all

all: Makefile $(SUBTARGETS)

mythnews/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "mythnews" || $(MKDIR) "mythnews"
	cd mythnews && $(QMAKE) mythnews.pro -o $(MAKEFILE)
sub-mythnews: mythnews/$(MAKEFILE) FORCE
	cd mythnews && $(MAKE) -f $(MAKEFILE)

Makefile: mythnews.pro  /usr/share/qt3/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile mythnews.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile mythnews.pro

all: $(SUBTARGETS)
qmake_all: mythnews/$(MAKEFILE)
	 ( [ -d mythnews ] && cd mythnews ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall uicables mocables install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d mythnews ] && cd mythnews ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d mythnews ] && cd mythnews ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

FORCE:

