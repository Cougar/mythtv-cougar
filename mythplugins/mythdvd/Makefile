#############################################################################
# Makefile for building: mythdvd
# Generated by qmake (1.04a) (Qt 3.1.1) on: Mon Jul 28 11:30:54 2003
# Project:  mythdvd.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile mythdvd.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-mythdvd \
		sub-mtd

first: all

all: Makefile $(SUBTARGETS)

mythdvd/$(MAKEFILE): 
	cd mythdvd && $(QMAKE)  -o $(MAKEFILE)
sub-mythdvd: mythdvd/$(MAKEFILE) FORCE
	cd mythdvd && $(MAKE) -f $(MAKEFILE)

mtd/$(MAKEFILE): 
	cd mtd && $(QMAKE)  -o $(MAKEFILE)
sub-mtd: mtd/$(MAKEFILE) FORCE
	cd mtd && $(MAKE) -f $(MAKEFILE)

Makefile: mythdvd.pro  $(QTDIR)/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile mythdvd.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile mythdvd.pro

all: $(SUBTARGETS)
qmake_all: mythdvd/$(MAKEFILE) mtd/$(MAKEFILE)
	 ( [ -d mythdvd ] && cd mythdvd ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d mtd ] && cd mtd ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d mythdvd ] && cd mythdvd ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d mtd ] && cd mtd ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d mythdvd ] && cd mythdvd ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d mtd ] && cd mtd ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

FORCE:

