MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-mythgame

first: all

all: Makefile $(SUBTARGETS)

mythgame/$(MAKEFILE): 
	cd mythgame && $(QMAKE)  -o $(MAKEFILE)
sub-mythgame: mythgame/$(MAKEFILE) FORCE
	cd mythgame && $(MAKE) -f $(MAKEFILE)

Makefile: mythgame.pro  $(QTDIR)/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile mythgame.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile mythgame.pro

all: $(SUBTARGETS)
qmake_all: mythgame/$(MAKEFILE)
	 ( [ -d mythgame ] && cd mythgame ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d mythgame ] && cd mythgame ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d mythgame ] && cd mythgame ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

FORCE:

