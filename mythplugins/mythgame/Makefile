#############################################################################
# Makefile for building: mythgame
# Generated by qmake (1.03a) on: Fri Sep 27 15:10:21 2002
# Project:  mythgame.pro
# Template: subdirs
# Command: $(QMAKE) mythgame.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
SUBDIRS  =	mythgame
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-mythgame

first: all

all: Makefile $(SUBTARGETS)

mythgame/$(MAKEFILE): 
	cd mythgame && $(QMAKE) -o $(MAKEFILE)
sub-mythgame: mythgame/$(MAKEFILE) FORCE
	cd mythgame && $(MAKE) -f $(MAKEFILE)

Makefile: mythgame.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) mythgame.pro
qmake: qmake_all
	@$(QMAKE) mythgame.pro

qmake_all: mythgame/$(MAKEFILE)
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; grep "^qmake_all:" $(MAKEFILE) 2>/dev/null >/dev/null && $(MAKE) -f $(MAKEFILE) qmake_all || true; fi; ) ; done

clean: qmake_all FORCE
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) -f $(MAKEFILE) clean; fi; ) ; done
uninstall install uiclean mocclean: qmake_all FORCE
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) -f $(MAKEFILE) $@; fi; ) ; done
distclean: qmake_all FORCE
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) -f $(MAKEFILE) $@ ; $(DEL_FILE) $(MAKEFILE) ; fi; ) ; done

FORCE:

