#############################################################################
# Makefile for building: mythgallery
# Generated by qmake (1.03a) on: Tue Oct 15 00:36:00 2002
# Project:  mythgallery.pro
# Template: subdirs
# Command: $(QMAKE) mythgallery.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
SUBDIRS  =	mythgallery
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-mythgallery

first: all

all: Makefile $(SUBTARGETS)

mythgallery/$(MAKEFILE): 
	cd mythgallery && $(QMAKE) -o $(MAKEFILE)
sub-mythgallery: mythgallery/$(MAKEFILE) FORCE
	cd mythgallery && $(MAKE) -f $(MAKEFILE)

Makefile: mythgallery.pro  $(QTDIR)/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) mythgallery.pro
qmake: qmake_all
	@$(QMAKE) mythgallery.pro

qmake_all: mythgallery/$(MAKEFILE)
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; grep "^qmake_all:" $(MAKEFILE) 2>/dev/null >/dev/null && $(MAKE) -f $(MAKEFILE) qmake_all || true; fi; ) ; done

clean: qmake_all FORCE
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) -f $(MAKEFILE) clean; fi; ) ; done
uninstall install uiclean mocclean: qmake_all FORCE
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) -f $(MAKEFILE) $@; fi; ) ; done
distclean: qmake_all FORCE
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) -f $(MAKEFILE) $@ ; $(DEL_FILE) $(MAKEFILE) ; fi; ) ; done

FORCE:

