MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-mythgallery \
		sub-i18n

first: all

all: Makefile $(SUBTARGETS)

mythgallery/$(MAKEFILE): 
	cd mythgallery && $(QMAKE)  -o $(MAKEFILE)
sub-mythgallery: mythgallery/$(MAKEFILE) FORCE
	cd mythgallery && $(MAKE) -f $(MAKEFILE)

i18n/$(MAKEFILE): 
	cd i18n && $(QMAKE)  -o $(MAKEFILE)
sub-i18n: i18n/$(MAKEFILE) FORCE
	cd i18n && $(MAKE) -f $(MAKEFILE)

Makefile: mythgallery.pro  $(QTDIR)/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile mythgallery.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile mythgallery.pro

all: $(SUBTARGETS)
qmake_all: mythgallery/$(MAKEFILE) i18n/$(MAKEFILE)
	 ( [ -d mythgallery ] && cd mythgallery ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d i18n ] && cd i18n ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d mythgallery ] && cd mythgallery ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d i18n ] && cd i18n ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d mythgallery ] && cd mythgallery ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d i18n ] && cd i18n ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

FORCE:

