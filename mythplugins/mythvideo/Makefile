# Makefile for building: mythvideo
# Generated by qmake (1.02a) on: Sat Aug 30 16:02:26 2003
# Project:  mythvideo.pro
# Template: subdirs
# Command: $(QMAKE) mythvideo.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE =	$(QTDIR)/bin/qmake
SUBDIRS =	mythvideo
SUBTARGETS =	 \
		sub-mythvideo

first: all

all: Makefile $(SUBTARGETS)

mythvideo/$(MAKEFILE): 
	cd mythvideo && $(QMAKE) -o $(MAKEFILE)
sub-mythvideo: mythvideo/$(MAKEFILE) FORCE
	cd mythvideo && $(MAKE) -f $(MAKEFILE)

Makefile: mythvideo.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) mythvideo.pro
qmake: qmake_all
	@$(QMAKE) mythvideo.pro

qmake_all: mythvideo/$(MAKEFILE)
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; grep "^qmake_all:" $(MAKEFILE) 2>/dev/null >/dev/null && $(MAKE) -f $(MAKEFILE) qmake_all || true; fi; ) ; done

clean: qmake_all FORCE
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) -f $(MAKEFILE) clean; fi; ) ; done
uninstall install uiclean mocclean: qmake_all FORCE
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) -f $(MAKEFILE) $@; fi; ) ; done
distclean: qmake_all FORCE
	for i in $(SUBDIRS); do ( if [ -d $$i ]; then cd $$i ; $(MAKE) -f $(MAKEFILE) $@ ; rm -f $(MAKEFILE) ; fi; ) ; done

FORCE:

