MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-mythvideo

first: all

all: Makefile $(SUBTARGETS)

mythvideo/$(MAKEFILE): 
	cd mythvideo && $(QMAKE)  -o $(MAKEFILE)
sub-mythvideo: mythvideo/$(MAKEFILE) FORCE
	cd mythvideo && $(MAKE) -f $(MAKEFILE)

Makefile: mythvideo.pro  $(QTDIR)/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile mythvideo.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile mythvideo.pro

all: $(SUBTARGETS)
qmake_all: mythvideo/$(MAKEFILE)
	 ( [ -d mythvideo ] && cd mythvideo ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d mythvideo ] && cd mythvideo ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d mythvideo ] && cd mythvideo ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

FORCE:

