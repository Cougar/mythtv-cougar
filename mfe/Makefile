# Makefile for building: mfe
# Generated by qmake (1.05a) (Qt 3.1.2) on: Mon Mar 15 14:41:09 2004
# Project:  mfe.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile mfe.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-mfe

first: all

all: Makefile $(SUBTARGETS)

mfe/$(MAKEFILE): 
	cd mfe && $(QMAKE)  -o $(MAKEFILE)
sub-mfe: mfe/$(MAKEFILE) FORCE
	cd mfe && $(MAKE) -f $(MAKEFILE)

Makefile: mfe.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile mfe.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile mfe.pro

all: $(SUBTARGETS)
qmake_all: mfe/$(MAKEFILE)
	 ( [ -d mfe ] && cd mfe ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d mfe ] && cd mfe ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d mfe ] && cd mfe ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

FORCE:

