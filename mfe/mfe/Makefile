		mfdinfo.h

mfdinfo.o: mfdinfo.cpp mfdinfo.h

moc_mfedialog.o: moc_mfedialog.cpp mfedialog.h mfdinfo.h

moc_mfedialog.cpp: $(MOC) mfedialog.h
	$(MOC) mfedialog.h -o moc_mfedialog.cpp

####### Install

install_target: 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)../../../../lib/mythtv/plugins/" || $(MKDIR) "$(INSTALL_ROOT)../../../../lib/mythtv/plugins/"
	-$(COPY) "$(TARGET)" "$(INSTALL_ROOT)../../../../lib/mythtv/plugins/$(TARGET)"
	-strip --strip-unneeded "$(INSTALL_ROOT)../../../../lib/mythtv/plugins/$(TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)../../../../lib/mythtv/plugins/$(TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)../../../../lib/mythtv/plugins/"


install_uifiles: 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)../../../../share/mythtv/themes/default/" || $(MKDIR) "$(INSTALL_ROOT)../../../../share/mythtv/themes/default/"
	-$(COPY_FILE) "mfe-ui.xml" "$(INSTALL_ROOT)../../../../share/mythtv/themes/default/"


uninstall_uifiles: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)../../../../share/mythtv/themes/default/mfe-ui.xml"
	-$(DEL_DIR) "$(INSTALL_ROOT)../../../../share/mythtv/themes/default/"


install: all install_target install_uifiles 

uninstall: uninstall_target uninstall_uifiles 

