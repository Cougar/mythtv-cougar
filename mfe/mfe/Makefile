CXXFLAGS = -pipe -Wall -W -O3 -march=pentiumpro -fomit-frame-pointer -D_REENTRANT -fPIC  -D_GNU_SOURCE -DPREFIX=\"/usr/local\" -DQT_NO_DEBUG -DQT_THREAD_SUPPORT -DQT_PLUGIN -DQT_SHARED
LEXFLAGS = 
YACCFLAGS= -d
